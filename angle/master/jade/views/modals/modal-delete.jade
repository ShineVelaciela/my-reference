script( type="text/ng-template",id="deleteTpl.html")
    div.modal-header
        button.close(type="button",ng-click="cancel();") ×
        h4.modal-title(id="myModalLabel") 批量删除
    div.modal-body
        form.form-horizontal
            div.form-group
                div.radio.c-radio.c-radio-nofont(style="float: left;margin-left: 15px;")
                    label
                        input(type="radio",value="1",ng-model="export.type")
                        span
                label.control-label 按选中删除
            div.form-group
                div.radio.c-radio.c-radio-nofont(style="float: left;margin-left: 15px;")
                    label
                        input(type="radio",value="2",ng-model="export.type")
                        span
                label.control-label 按序号删除
                label.control-label.col-sm-1 起
                div.col-sm-2
                    input.form-control.input-sm(type="text",ng-model="export.startNo")
                label.control-label.col-sm-1 止
                div.col-sm-2
                    input.form-control.input-sm(type="text",ng-model="export.endNo")
            div.form-group
                div.radio.c-radio.c-radio-nofont(style="float: left;margin-left: 15px;")
                    label
                        input(type="radio",value="3",ng-model="export.type")
                        span
                label.control-label 按页数删除
                label.control-label.col-sm-1 起
                div.col-sm-2
                    input.form-control.input-sm(type="text",ng-model="export.pageStart")
                label.control-label.col-sm-1 止
                div.col-sm-2
                    input.form-control.input-sm(type="text",ng-model="export.pageEnd")
    div.modal-footer
        span(style="color: red;") {{error}}
        button.btn.btn-primary(type="button",ng-click="ok();") 确认
        button.btn.btn-warning(type="button",ng-click="cancel();") 取消